{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "es6": true,
  },
  "rules": {
    "no-console": 1,
    "no-unused-vars": 1,
    "react/no-multi-comp": [0, {"ignoreStateless": true }],
    "react/prop-types": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  },
  "plugins": ["html", "markdown"]
}
